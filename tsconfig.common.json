{
  "compilerOptions": {
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "strict": true,
    "strictNullChecks": false,
    "skipLibCheck": true,
    "removeComments": true,
    "sourceMap": true,
    "declaration": true,
    "lib": [
      "ESNext"
    ],
    "module": "commonjs",
    "target": "ES2020",
    "experimentalDecorators": true,
    "pretty": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "forceConsistentCasingInFileNames": true
  }
}
